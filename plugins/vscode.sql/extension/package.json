{
  "name": "sql",
  "displayName": "SQL Language Basics (built-in)",
  "description": "Provides syntax highlighting and bracket matching in SQL files.",
  "version": "1.53.2",
  "publisher": "vscode",
  "license": "SEE LICENSE IN LICENSE-vscode.txt",
  "engines": {
    "vscode": "*"
  },
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "sql",
        "extensions": [
          ".sql",
          ".dsql"
        ],
        "aliases": [
          "SQL"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sql",
        "scopeName": "source.sql",
        "path": "./syntaxes/sql.tmLanguage.json"
      }
    ]
  },
  "keywords": [
    "Built-in"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/vscode-builtin-extensions"
  }
}