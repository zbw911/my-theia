{
  "name": "hlsl",
  "displayName": "HLSL Language Basics (built-in)",
  "description": "Provides syntax highlighting and bracket matching in HLSL files.",
  "version": "1.53.2",
  "publisher": "vscode",
  "license": "SEE LICENSE IN LICENSE-vscode.txt",
  "engines": {
    "vscode": "*"
  },
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "hlsl",
        "extensions": [
          ".hlsl",
          ".hlsli",
          ".fx",
          ".fxh",
          ".vsh",
          ".psh",
          ".cginc",
          ".compute"
        ],
        "aliases": [
          "HLSL",
          "hlsl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hlsl",
        "path": "./syntaxes/hlsl.tmLanguage.json",
        "scopeName": "source.hlsl"
      }
    ]
  },
  "keywords": [
    "Built-in"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/vscode-builtin-extensions"
  }
}