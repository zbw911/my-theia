{
  "name": "shaderlab",
  "displayName": "Shaderlab Language Basics (built-in)",
  "description": "Provides syntax highlighting and bracket matching in Shaderlab files.",
  "version": "1.53.2",
  "publisher": "vscode",
  "license": "SEE LICENSE IN LICENSE-vscode.txt",
  "engines": {
    "vscode": "*"
  },
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "shaderlab",
        "extensions": [
          ".shader"
        ],
        "aliases": [
          "ShaderLab",
          "shaderlab"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "shaderlab",
        "path": "./syntaxes/shaderlab.tmLanguage.json",
        "scopeName": "source.shaderlab"
      }
    ]
  },
  "keywords": [
    "Built-in"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/vscode-builtin-extensions"
  }
}